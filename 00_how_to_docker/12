#!/bin/bash
docker run -d \
    --restart on-failure \
    --name spawning-pool \
    -e MYSQL_ROOT_PASSWORD=Kerrigan \
    -e MYSQL_DATABASE=zerglings \
    -v hatchery:/var/lib/mysql \
mysql:8.0 \

